{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["postgresql"],
      "allowedVersions": "<17"
    },
    {
      "matchDatasources": ["github-releases"],
      "matchPackagePatterns": ["kubelet"],
      "allowedVersions": "<1.32.0"
    }
  ]
}